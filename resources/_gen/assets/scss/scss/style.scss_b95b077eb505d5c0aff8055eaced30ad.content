-style:italic}body{font-family:opensans}h1,h2,h3,h4,h5,h6{font-family:quicksand}p{text-align:justify;line-height:1.5rem}footer{padding:30px 0 20px;background-color:#eee}.btn-hero{color:#fff;margin-bottom:30px;border-radius:30px;font-size:18px;font-weight:700;padding:15px 40px;background-color:#48d}.btn-hero:hover,.btn-hero:focus{box-shadow:0 5px 25px #48d9;color:#fff}.library{display:none}.library .library-slider{padding-top:50px;padding-bottom:30px}.library .library-slider a{color:inherit;text-decoration:none}.library .library-slider img{margin-bottom:10px}.library .library-slide{padding:20px;position:relative}.library .library-slide:hover .library-slide-overlay{transform:scale(1);opacity:1;border-radius:0}.library .library-slide .library-slide-overlay{position:absolute;bottom:15px;left:15px;right:15px;top:15px;background:#fff9;text-decoration:none;transform:scale(0.7);transition:.3s ease-in-out;z-index:2;opacity:0;border-radius:50%}.library .library-slide .library-slide-overlay .library-slide-popup{text-align:center;position:absolute;top:40%;left:50%;transform:translate(-50%,-50%);background:#48d;padding:5px 10px;border-radius:40px;width:50px;height:50px;color:#fff;font-size:1.66rem;line-height:2.5rem}.library .library-slide .library-slide-label{font-weight:700}.section-banner p.subtext{font-size:.8rem;line-height:1.2rem}.section-proposition{background-attachment:fixed;background-repeat:no-repeat;background-size:cover;background-position:50%;box-shadow:inset 0 0 0 1000px rgba(0,0,0,.5);padding:75px 0}.section-promo{padding:75px;background-color:#48d;color:#fff}.section-testimonials{padding:75px 0}.section-testimonials img{height:60px;border-radius:30px}